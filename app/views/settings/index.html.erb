<%= form_with(model: @settings) do |f|%>
<%= f.label :pseudo %>
<%= f.text_field :pseudo%>
<br /> <br />
<%= f.label :description %>
<%= f.text_field :description%>
<br /> <br />
<%= f.file_field :avatar, { placeholder: "Image du produit" } %>
<br /> <br />
<%= f.submit "Send"%>
<% end %>

